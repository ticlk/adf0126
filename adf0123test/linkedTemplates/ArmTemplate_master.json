{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf0123test"
		},
		"ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS'"
		},
		"SQLDB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQLDB_LS'"
		},
		"ADLS_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagee1203.dfs.core.chinacloudapi.cn/"
		},
		"SQLDB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql1111.database.chinacloudapi.cn"
		},
		"SQLDB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db"
		},
		"SQLDB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "lihong"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf0123test_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LS_accountKey": {
						"value": "[parameters('ADLS_LS_accountKey')]"
					},
					"SQLDB_LS_password": {
						"value": "[parameters('SQLDB_LS_password')]"
					},
					"ADLS_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LS_properties_typeProperties_url')]"
					},
					"SQLDB_LS_properties_typeProperties_server": {
						"value": "[parameters('SQLDB_LS_properties_typeProperties_server')]"
					},
					"SQLDB_LS_properties_typeProperties_database": {
						"value": "[parameters('SQLDB_LS_properties_typeProperties_database')]"
					},
					"SQLDB_LS_properties_typeProperties_userName": {
						"value": "[parameters('SQLDB_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}